{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        ["babel-plugin-styled-components", { "ssr": false,"fileName": true, "displayName": true, "pure": true }],
        "@babel/plugin-proposal-class-properties",
        "react-hot-loader/babel"
    ],
    "env": {
        "production": {
            "presets": [
                "minify"
            ]
        }
    }
}
